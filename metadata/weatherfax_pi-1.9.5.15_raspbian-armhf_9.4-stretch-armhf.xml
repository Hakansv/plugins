<?xml version="1.0" encoding="UTF-8"?>
<plugin version="1">
  <name> WeatherFax </name>
  <version> 1.9.5.15 </version>
  <release> 15 </release>
  <summary> Open fax image files or decode audio faxes and overlay on chart </summary>

  <api-version> 1.16 </api-version>
  <open-source> yes </open-source>
  <author> Sean d'Epagnier </author>
  <source> https://github.com/rgleason/weatherfax_pi </source>

  <description>
    Simple image overlay calibration, Converts any image image into a raster chart.  Db for HF radio fax stations via SSB and Metro URL internet retrieval.
  </description>

  <target>raspbian</target>
  <target-version>9.4</target-version>
  <target-arch>armhf</target-arch>
  <tarball-url>
    https://dl.cloudsmith.io/public/rick-gleason/weatherfax-prod/raw/names/weatherfax_pi-1.9.5.15-raspbian-armhf_9.4-stretch-armhf-tarball/versions/v1.9.5.15/weatherfax_pi-1.9.5.15_raspbian-armhf_9.4-stretch-armhf.tar.gz
  </tarball-url>
  <info-url> https://opencpn.org/OpenCPN/plugins/weatherfax.html </info-url>
</plugin>
